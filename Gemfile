source 'https://rubygems.org'

gem 'rails', '~> 3.2.13'
gem 'mysql2', '~> 0.3.11'


# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails', '~> 3.2.3'
  gem 'bootstrap-sass', '~> 2.3.1.0'
  gem 'coffee-rails', '~> 3.2.1'
  gem 'uglifier', '>= 1.0.3'
end

group :development, :test do
	gem 'pry'
	gem 'rspec-rails'
	gem 'guard-rspec'
end

group :test do
  gem 'rb-inotify'
end

gem 'jquery-rails'
